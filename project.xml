?xml version="1.0" encoding="utf-8"?>
<project>
    
    <meta title="Go Play Smile" version="1.0.0" />
    <app main="Server" path="Export" file="GPS" />
    
    <window width="400" height="240" fps="61" resizable="false" />

    <haxelib name="extension-iap" />
    <haxelib name="openfl" />
    <icon path="assets/etc/app.png" />
    <assets path="Assets" rename="assets" />

    <haxedef name="HXCPP_CHECK_POINTER"/>  <!--makes null references cause errors-->
    <haxedef name="HXCPP_STACK_LINE" />    <!--if you want line numbers-->
    <haxedef name="HXCPP_STACK_TRACE"/>    <!--if you want stack traces-->
    
</project>